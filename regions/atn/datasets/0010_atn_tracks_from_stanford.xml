<dataset type="EDDTableFromNcFiles" datasetID="stanford_import_ssm_public">
    <reloadEveryNMinutes>10080</reloadEveryNMinutes>
    <updateEveryNMillis>10000</updateEveryNMillis>
    <fileDir>/mnt/store/data/atn/stanford_import/ssm/public</fileDir>
    <fileNameRegex>.*\.nc</fileNameRegex>
    <recursive>true</recursive>
    <pathRegex>.*</pathRegex>
    <metadataFrom>last</metadataFrom>
    <preExtractRegex/>
    <postExtractRegex/>
    <extractRegex/>
    <columnNameForExtract/>
    <sortedColumnSourceName>time</sortedColumnSourceName>
    <sortFilesBySourceNames>time</sortFilesBySourceNames>
    <fileTableInMemory>false</fileTableInMemory>
    <accessibleViaFiles>true</accessibleViaFiles>
    <addAttributes>
      <att name="title">IOOS ATN Trackline Database from Core 1 Dump from the Stanford Block Lab"</att>
      <att name="summary">Animal tracklines as a product of processing and cross referencing data in the Core 1/2 IOOS ATN databases"</att>
      <att name="cdm_data_type">Trajectory</att>
      <att name="cdm_trajectory_variables">genus, common_name, species, taxon, event_id, topp_id, deploy_year, workgroup</att>
      <att name="subsetVariables">genus, common_name, species, taxon, event_id, topp_id, deploy_year, workgroup</att>
      <att name="Conventions">CF-1.6, ACDD-1.3</att>
      <att name="keywords">ioos, argos, atn, dac, data, deployed, event, profile, telemetry</att>
      <att name="Metadata_Conventions">COARDS, CF-1.6, Unidata Dataset Discovery v1.0</att>
      <att name="standard_name_vocabulary">CF Standard Name Table v57</att>
      <att name="sourceUrl">(local files)</att>
    </addAttributes>
    <dataVariable>
      <sourceName>lat</sourceName>
      <destinationName>latitude</destinationName>
      <dataType>float</dataType>
      <addAttributes>
        <att name="colorBarMaximum" type="double">90.0</att>
        <att name="colorBarMinimum" type="double">-90.0</att>
        <att name="_FillValue" type="float">-9999</att>
        <att name="missing_value" type="float">-9999</att>
      </addAttributes>
    </dataVariable>
    <dataVariable>
      <sourceName>lon</sourceName>
      <destinationName>longitude</destinationName>
      <dataType>float</dataType>
      <addAttributes>
        <att name="colorBarMaximum" type="double">180.0</att>
        <att name="colorBarMinimum" type="double">-180.0</att>
        <att name="_FillValue" type="float">-9999</att>
        <att name="missing_value" type="float">-9999</att>
      </addAttributes>
    </dataVariable>
    <dataVariable>
      <sourceName>time</sourceName>
      <destinationName>time</destinationName>
      <dataType>int</dataType>
      <addAttributes>
        <att name="_FillValue" type="int">-9999</att>
        <att name="missing_value" type="int">-9999</att>
      </addAttributes>
    </dataVariable>
    <dataVariable>
      <sourceName>global:genus</sourceName>
      <destinationName>genus</destinationName>
      <dataType>String</dataType>
      <addAttributes>
        <att name="ioos_category">Taxonomy</att>
      </addAttributes>
    </dataVariable>
    <dataVariable>
      <sourceName>global:common_name</sourceName>
      <destinationName>common_name</destinationName>
      <dataType>String</dataType>
      <addAttributes>
        <att name="ioos_category">Taxonomy</att>
      </addAttributes>
    </dataVariable>
    <dataVariable>
      <sourceName>global:species</sourceName>
      <destinationName>species</destinationName>
      <dataType>String</dataType>
      <addAttributes>
        <att name="ioos_category">Fish Species</att>
      </addAttributes>
    </dataVariable>
    <dataVariable>
      <sourceName>global:id</sourceName>
      <destinationName>event_id</destinationName>
      <dataType>int</dataType>
      <addAttributes>
        <att name="ioos_category">Other</att>
        <att name="cf_role">trajectory_id</att>
      </addAttributes>
    </dataVariable>
    <dataVariable>
      <sourceName>global:workgroup</sourceName>
      <destinationName>workgroup</destinationName>
      <dataType>String</dataType>
      <addAttributes>
        <att name="ioos_category">Other</att>
      </addAttributes>
    </dataVariable>
    <dataVariable>
      <sourceName>global:topp_id</sourceName>
      <destinationName>topp_id</destinationName>
      <dataType>int</dataType>
      <addAttributes>
        <att name="ioos_category">Other</att>
      </addAttributes>
    </dataVariable>
    <dataVariable>
      <sourceName>global:deploy_year</sourceName>
      <destinationName>deploy_year</destinationName>
      <dataType>int</dataType>
      <addAttributes>
        <att name="ioos_category">Other</att>
      </addAttributes>
    </dataVariable>
    <dataVariable>
      <sourceName>global:taxon</sourceName>
      <destinationName>taxon</destinationName>
      <dataType>String</dataType>
      <addAttributes>
        <att name="ioos_category">Other</att>
      </addAttributes>
    </dataVariable>
    <dataVariable>
      <sourceName>global:time_coverage_start</sourceName>
      <destinationName>time_coverage_start</destinationName>
      <dataType>String</dataType>
      <addAttributes>
        <att name="ioos_category">Time</att>
        <att name="units">yyyy-MM-dd'T'HH:mm:ssZ</att>
      </addAttributes>
    </dataVariable>
    <dataVariable>
      <sourceName>global:time_coverage_end</sourceName>
      <destinationName>time_coverage_end</destinationName>
      <dataType>String</dataType>
      <addAttributes>
        <att name="ioos_category">Time</att>
        <att name="units">yyyy-MM-dd'T'HH:mm:ssZ</att>
      </addAttributes>
    </dataVariable>
    <dataVariable>
      <sourceName>global:deployment_date</sourceName>
      <destinationName>deployment_date</destinationName>
      <dataType>String</dataType>
      <addAttributes>
        <att name="ioos_category">Time</att>
        <att name="units">yyyy-MM-dd HH:mm:ssZ</att>
      </addAttributes>
    </dataVariable>
  </dataset>
