<dataset type="EDDTableFromAsciiFiles" datasetID="ioos_atn_contacts" active="true">
    <reloadEveryNMinutes>10080</reloadEveryNMinutes>
    <fileDir>/mnt/store/data/atn/stanford_import/</fileDir>
    <fileNameRegex>contacts.csv</fileNameRegex>
    <columnNamesRow>1</columnNamesRow>
    <addAttributes>
        <att name="acknowledgement">NOAA IOOS,Axiom Data Science,Wildlife Computers,Argos,SMRU,Navy ONR,NOAA NMFS,Stanford Block Lab</att>
        <att name="cdm_data_type">Other</att>
        <att name="Conventions">CF-1.6,ACDD-1.3</att>
        <att name="creator_email">atn@ioos.us</att>
        <att name="creator_name">IOOS ATN</att>
        <att name="creator_url">https://atn.ioos.us</att>
        <att name="info_url">https://atn.ioos.us</att>
        <att name="infoUrl">https://atn.ioos.us</att>
        <att name="institution">IOOS Animal Telemetry Network</att>
        <att name="keywords">ioos,atn,dac,data,deployed,event,contact</att>
        <att name="license">[standard]</att>
        <att name="project">IOOS Animal Telemetry Network</att>
        <att name="publisher_email">atn@ioos.us</att>
        <att name="publisher_name">IOOS ATN</att>
        <att name="publisher_url">https://atn.ioos.us</att>
        <att name="sourceUrl">(local files)</att>
        <att name="subsetVariables">event_id,contact_email,contact_name,institution</att>
        <att name="summary">Extracted contact information from the Hopkins Marine Lab ATN data dump. This is useful when looking up contact information by an event_id</att>
        <att name="title">IOOS ATN Contact Database</att>
    </addAttributes>
    <dataVariable>
        <sourceName>event_id</sourceName>
        <destinationName>event_id</destinationName>
        <dataType>String</dataType>
        <addAttributes>
            <att name="ioos_category">Other</att>
            <att name="description">This unique event_id describing a telemetry event in the IOOS ATN DAC</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>contact_name</sourceName>
        <destinationName>contact_name</destinationName>
        <dataType>String</dataType>
        <addAttributes>
            <att name="ioos_category">Other</att>
            <att name="description">The contact name of the person who was responsible for the deployment and management of the telemery event and data prior to DAC submission</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>contact_email</sourceName>
        <destinationName>contact_email</destinationName>
        <dataType>String</dataType>
        <addAttributes>
            <att name="ioos_category">Other</att>
            <att name="description">The contact email of the person who was responsible for the deployment and management of the telemery event and data prior to DAC submission</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>institution</sourceName>
        <destinationName>institution</destinationName>
        <dataType>String</dataType>
        <addAttributes>
            <att name="ioos_category">Other</att>
           <att name="description">The institution that was responsible for the deployment and management of the telemery event and data prior to DAC submission</att>
        </addAttributes>
    </dataVariable>
</dataset>
